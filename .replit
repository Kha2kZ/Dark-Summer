modules = ["java-graalvm22.3", "bash"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["maven"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "build-plugin"

[[workflows.workflow]]
name = "build-plugin"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "mvn clean package"

[[workflows.workflow]]
name = "ðŸ’¾ Backup to GitHub"
author = 49635968
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git add ."

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "git commit -m \"Backup: $(date '+%Y-%m-%d %H:%M:%S')\""

[[workflows.workflow.tasks]]
